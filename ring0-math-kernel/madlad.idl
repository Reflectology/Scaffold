// Common Data Structures
dictionary OmegaState {
    DOMString id;
    double timestamp;
    object data;
    sequence<long> axioms_satisfied;
    double cost;
    double entropy;
    long dimension;
};

dictionary MathResult {
    double value;
    sequence<long> axiom_trace;
    boolean convergence;
    double error;
};

